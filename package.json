{
  "name": "git-cheatsheet",
  "version": "1.0.0",
  "description": "Git Cheatsheet",
  "main": "git-cheatsheet.html",
  "scripts": {
    "build": "yarn build-js && yarn build-styles",
    "build-js": "babel --source-maps --compact true -o git-cheatsheet/main.js node_modules/jquery/dist/jquery.min.js src/lib/rx.lite.js src/base.js src/commands.js src/cookies.js src/git-cheatsheet.js",
    "build-styles": "node ./src/build-styles.js",
    "clean": "rm -f git-cheatsheet/*.map git-cheatsheet/*.js ./git-cheatsheet/styles.css",
    "test": "jasmine spec/*_spec.js",
    "start": "static -p 9090"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ndp/git-cheatsheet.git"
  },
  "author": "Andrew J. Peterson / NDP Software",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ndp/git-cheatsheet/issues"
  },
  "homepage": "https://github.com/ndp/git-cheatsheet#readme",
  "devDependencies": {
    "@babel/cli": "*",
    "@babel/core": "*",
    "@babel/preset-env": "*",
    "jasmine": "*",
    "jquery": "^3.6.0",
    "node-static": "*"
  },
  "dependencies": {}
}
